<main>
  <h1>Browser</h1>
  <form action="" method="">
    <input type="text" class="search" value="{{search}}" name="search" placeholder="Search...">
    <input type="number" name="from" value="{{from}}" class="difficulty" placeholder="Difficulty from...">
    <span>-</span>
    <input type="number" name="to" value="{{to}}" class="difficulty" placeholder="Difficulty to...">
    <input type="submit" value="search">
  </form>

  {{#each cubes}}
  <div class="cube">
    <img class="cube" src="{{imageURL}}">
    <p class="name">{{name}}</p>
    <p><span>Difficulty level:</span>{{difficultyLevel}}</p>
    <a class="btn" href="/details/{{id}}">Details</a>
  </div>
  {{/each}}

</main>